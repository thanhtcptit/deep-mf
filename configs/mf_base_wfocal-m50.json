{
    "seed": 442,
    "model": {
        "type": "mf",
        "latent_dim": 32,
        "l2_reg": 0.01,
        "unit_norm_emb": false,
        "use_bias": false,
        "act": "sigmoid"
    },
    "trainer": {
        "num_steps": 200000,
        "batch_size": 2560,
        "mode": "train_jointly",
        "loss_fn": {
            "type": "wfocal",
            "multiply": 50
        },
        "optimizer": {
            "type": "sgd",
            "learning_rate": 2.0
        },
        "grad_clip": {
            "min_value": -2,
            "max_value": 2
        },
        "display_step": 100,
        "save_step": 5000,
        "validate_step": 5000
    },
    "hyp": {
        "trainer.loss_fn.multiply": {
            "type": "categorical",
            "values": [20, 50, 100, 200, 500]
        }
    }
}