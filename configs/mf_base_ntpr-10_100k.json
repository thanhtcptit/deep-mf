{
    "seed": 442,
    "data": {
        "path": {
            "raw": "data/raw/100k",
            "processed": "data/processed/100k_ntpr-10"
        },
        "neg_to_pos_ratio": 10
    },
    "model": {
        "type": "mf",
        "latent_dim": 32,
        "l2_reg": 0.01,
        "unit_norm_emb": false,
        "use_bias": true,
        "act": "sigmoid"
    },
    "trainer": {
        "num_steps": 200000,
        "batch_size": 2560,
        "mode": "train_jointly",
        "loss_fn": {
            "type": "wbce",
            "multiply": 2
        },
        "optimizer": {
            "type": "sgd",
            "learning_rate": 2.5
        },
        "display_step": 100,
        "save_step": 5000,
        "validate_step": 5000
    },
    "hyp": {

    }
}