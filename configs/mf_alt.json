{
    "seed": 442,
    "data": {
        "path": {
            "raw": "data/raw/50k",
            "processed": "data/processed/50k"
        },
        "neg_to_pos_ratio": 0.5
    },
    "model": {
        "type": "mf",
        "latent_dim": 32,
        "l2_reg": 0.01,
        "unit_norm_emb": false,
        "use_bias": false,
        "act": "sigmoid"
    },
    "trainer": {
        "num_steps": 100000,
        "batch_size": 2560,
        "mode": "alt_training",
        "num_user_emb_train_step": 5000,
        "num_item_emb_train_step": 5000,
        "loss_fn": {
            "type": "wbce"
        },
        "optimizer": {
            "type": "sgd",
            "params": {
                "learning_rate": 1.5
            }
        },
        "display_step": 100,
        "save_step": 2000,
        "validate_step": 1000
    }
}